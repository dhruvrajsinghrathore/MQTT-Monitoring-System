  "sensor_descriptions": {
    "tumor_prolif_index": {
      "name": "Tumor Proliferation Index",
      "description": "Measures the rate of cell division and growth in tumor cells",
      "unit": "index",
      "range": "0.0 - 1.0",
      "normal_range": "0.4 - 0.8",
      "sensor_type": "imaging",
      "measurement_method": "Quantitative phase imaging analysis"
    },
    "glucose_mM": {
      "name": "Glucose Concentration",
      "description": "Measures glucose levels in the culture medium",
      "unit": "mM",
      "range": "0.0 - 25.0",
      "normal_range": "5.0 - 15.0",
      "sensor_type": "amperometric",
      "measurement_method": "Glucose oxidase electrode"
    },
    "aptamer_TNFa_nM": {
      "name": "TNF-α Concentration",
      "description": "Measures tumor necrosis factor alpha levels",
      "unit": "nM",
      "range": "0.0 - 5.0",
      "normal_range": "0.1 - 2.0",
      "sensor_type": "electrochemical",
      "measurement_method": "Aptamer-based biosensor"
    },
    "o2_percent": {
      "name": "Oxygen Saturation",
      "description": "Measures dissolved oxygen levels in the culture medium",
      "unit": "percent",
      "range": "40.0 - 100.0",
      "normal_range": "70.0 - 90.0",
      "sensor_type": "optical",
      "measurement_method": "Oxygen optode"
    },
    "barrier_impedance_kOhm": {
      "name": "Barrier Impedance",
      "description": "Measures electrical impedance across cell barrier",
      "unit": "kOhm",
      "range": "2.0 - 6.0",
      "normal_range": "3.0 - 5.0",
      "sensor_type": "impedance",
      "measurement_method": "Electrical impedance spectroscopy"
    },
    "pH": {
      "name": "pH Level",
      "description": "Measures acidity/alkalinity of the culture medium",
      "unit": "pH",
      "range": "6.8 - 7.6",
      "normal_range": "7.2 - 7.4",
      "sensor_type": "optical",
      "measurement_method": "pH optode"
    },
    "flow_uL_min": {
      "name": "Flow Rate",
      "description": "Measures fluid flow rate through the microchannel",
      "unit": "µL/min",
      "range": "0.0 - 20.0",
      "normal_range": "2.0 - 10.0",
      "sensor_type": "flow",
      "measurement_method": "Flow sensor"
    },
    "pressure_mbar": {
      "name": "Pressure",
      "description": "Measures fluid pressure in the microchannel",
      "unit": "mbar",
      "range": "30.0 - 200.0",
      "normal_range": "50.0 - 100.0",
      "sensor_type": "pressure",
      "measurement_method": "Pressure transducer"
    },
    "hcs_ROS_AU": {
      "name": "Reactive Oxygen Species",
      "description": "Measures reactive oxygen species levels in cells",
      "unit": "AU",
      "range": "0.0 - 5000.0",
      "normal_range": "500.0 - 2000.0",
      "sensor_type": "fluorescence",
      "measurement_method": "High-content screening fluorescence"
    },
    "hcs_Ca_ratio": {
      "name": "Calcium Ratio",
      "description": "Measures intracellular calcium levels",
      "unit": "ratio",
      "range": "0.5 - 2.0",
      "normal_range": "0.8 - 1.2",
      "sensor_type": "fluorescence",
      "measurement_method": "Calcium indicator fluorescence"
    },
    "hcs_viability_pct": {
      "name": "Cell Viability",
      "description": "Measures percentage of viable cells",
      "unit": "percent",
      "range": "0.0 - 100.0",
      "normal_range": "85.0 - 100.0",
      "sensor_type": "fluorescence",
      "measurement_method": "Live/dead cell staining"
    },
    "aptamer_IL6_nM": {
      "name": "IL-6 Concentration",
      "description": "Measures interleukin-6 levels",
      "unit": "nM",
      "range": "0.0 - 5.0",
      "normal_range": "0.1 - 2.0",
      "sensor_type": "electrochemical",
      "measurement_method": "Aptamer-based biosensor"
    },
    "qpi_cell_count": {
      "name": "Cell Count",
      "description": "Measures number of cells in the culture",
      "unit": "count",
      "range": "0 - 10000",
      "normal_range": "100 - 1000",
      "sensor_type": "imaging",
      "measurement_method": "Quantitative phase imaging"
    },
    "qpi_confluence_pct": {
      "name": "Cell Confluence",
      "description": "Measures percentage of surface area covered by cells",
      "unit": "percent",
      "range": "0.0 - 100.0",
      "normal_range": "20.0 - 80.0",
      "sensor_type": "imaging",
      "measurement_method": "Quantitative phase imaging"
    },
    "qpi_dry_mass_pg": {
      "name": "Cell Dry Mass",
      "description": "Measures dry mass of cells",
      "unit": "pg",
      "range": "100.0 - 500.0",
      "normal_range": "200.0 - 400.0",
      "sensor_type": "imaging",
      "measurement_method": "Quantitative phase imaging"
    },
    "tumor_EMT_index": {
      "name": "Epithelial-Mesenchymal Transition Index",
      "description": "Measures the degree of EMT in tumor cells",
      "unit": "index",
      "range": "0.0 - 1.0",
      "normal_range": "0.2 - 0.6",
      "sensor_type": "imaging",
      "measurement_method": "Quantitative phase imaging analysis"
    }
  }
